// Place your key bindings in this file to override the defaultsauto[]
[
    {
        "key": "cmd+;",
        "command": "expandLineSelection",
        "when": "textInputFocus"
    },
    {
        "key": "cmd+l",
        "command": "-expandLineSelection",
        "when": "textInputFocus"
    },
    {
        "key": "cmd+;",
        "command": "expandLineSelection",
        "when": "editorFocus"
    },
    {
        "key": "cmd+l",
        "command": "-expandLineSelection",
        "when": "editorFocus"
    },
    {
        "key": "alt+cmd+5",
        "command": "workbench.action.editorLayoutTwoByTwoGrid"
    },
    {
        "key": "cmd+j cmd+l",
        "command": "extension.runFileSpecs"
    },
    {
        "key": "shift+cmd+t",
        "command": "-extension.runFileSpecs"
    },
    {
        "key": "cmd+a",
        "command": "editor.action.selectAll"
    },
    {
        "key": "cmd+a",
        "command": "-editor.action.selectAll"
    }
]
